&lt;{{ element ?: 'div' }}{{ attributes.addClass(classes) }}&gt;
  {% if not label_hidden %}
    &lt;{{ label_element ?: 'h3' }}{{ title_attributes.addClass(title_classes) }}&gt;{{ label }}&lt;/{{ label_element ?: 'h3' }}&gt;
  {% endif %}
  {% if multiple %}
    &lt;div class=&quot;field__items&quot;&gt;
  {% endif %}
  {% for item in items %}
    &lt;div{{ item.attributes.addClass('field__item') }}&gt;{{ item.content }}&lt;/div&gt;
  {% endfor %}
  {% if multiple %}
    &lt;/div&gt;
  {% endif %}
&lt;/{{ element ?: 'div' }}&gt;
