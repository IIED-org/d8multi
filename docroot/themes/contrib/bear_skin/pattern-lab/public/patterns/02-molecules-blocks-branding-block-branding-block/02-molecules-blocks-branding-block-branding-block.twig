{% block content %}
  {% if site_logo %}
    &lt;a href=&quot;{{ path('&lt;front&gt;') }}&quot; title=&quot;{{ 'Home'|t }}&quot; rel=&quot;home&quot; class=&quot;branding__site-logo&quot;&gt;
      &lt;object type=&quot;image/svg+xml&quot; data=&quot;{{ site_logo }}&quot; class=&quot;logo&quot;&gt;
        &lt;img src=&quot;{{ site_logo }}&quot; alt=&quot;{{ 'Home'|t }}&quot; class=&quot;branding__site-logo-image&quot;/&gt;
      &lt;/object&gt;
    &lt;/a&gt;
  {% endif %}
  &lt;div class=&quot;branding__flex-container&quot;&gt;
  {% if site_name %}
    &lt;a href=&quot;{{ path('&lt;front&gt;') }}&quot; title=&quot;{{ 'Home'|t }}&quot; rel=&quot;home&quot; class=&quot;branding__site-name&quot;&gt;{{ site_name }}&lt;/a&gt;
  {% endif %}

  {% if site_slogan %}
    &lt;h6 class=&quot;branding__site-slogan&quot;&gt;{{ site_slogan }}&lt;/h6&gt;
  {% endif %}
  &lt;/div&gt;
{% endblock %}
