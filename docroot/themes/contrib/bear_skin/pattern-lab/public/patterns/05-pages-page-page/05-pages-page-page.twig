{% block region_header %}
  {{ region_header }}
{% endblock %}

{% block region_highlighted %}
&lt;div class=&quot;wrapper-highlighted&quot;&gt;
  &lt;div class=&quot;wrapper--inner wrapper-highlighted__inner&quot;&gt;
    {{ region_highlighted }}
  &lt;/div&gt;
&lt;/div&gt;
{% endblock %}

&lt;div class=&quot;wrapper-main {{main_class}}&quot;&gt;
  &lt;div class=&quot;wrapper--inner wrapper-main__inner&quot;&gt;

    &lt;div class=&quot;column main&quot; role=&quot;main&quot;&gt;

      {% block region_help %}
        {{ region_help }}
      {% endblock %}

      &lt;a id=&quot;main&quot;&gt;&lt;/a&gt;

      {% block region_content %}
        {{ region_content }}
      {% endblock %}

      {% if feed_icons %}
        {{ feed_icons }}
      {% endif %}

    &lt;/div&gt;

    {% block region_sidebar_first %}
      {{ region_sidebar_first }}
    {% endblock %}

    {% block region_sidebar_second %}
      {{ region_sidebar_second }}
    {% endblock %}
  &lt;/div&gt;
&lt;/div&gt;

{% block region_breadcrumb %}
&lt;div class=&quot;wrapper-breadcrumb&quot;&gt;
  &lt;div class=&quot;wrapper--inner wrapper-breadcrumb__inner&quot;&gt;
    {{ region_breadcrumb }}
  &lt;/div&gt;
&lt;/div&gt;
{% endblock %}

{% block region_footer %}
  {{ region_footer }}
{% endblock %}
